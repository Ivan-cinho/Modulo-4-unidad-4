<h1>{{title}}</h1>
{{#if conocido}}
    <p>Hola {{nombre}} {{apellido}}<a href="/salir">Salir</a></p>
{{/if}}

{{#unless conocido}}
    <p>Quien te conoce?</p>
    <form action="/ingresar" method="post">
        <input type="text" placeholder="nombre" name="nombre">
        <input type="text" placeholder="apellido" name="apellido"><button>ingresar</button>
    </form>
{{/unless}}